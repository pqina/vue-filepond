{
    "name": "vue-filepond",
    "version": "6.0.3",
    "description": "A handy FilePond adapter component for Vue",
    "homepage": "https://pqina.nl/filepond",
    "license": "MIT",
    "repository": "pqina/vue-filepond",
    "main": "dist/vue-filepond.umd.min.js",
    "browser": "dist/vue-filepond.umd.min.js",
    "module": "dist/vue-filepond.esm.js",
    "unpkg": "dist/vue-filepond.umd.min.js",
    "types": "types/index.d.ts",
    "files": [
        "dist",
        "types/*.d.ts"
    ],
    "keywords": [
        "vue",
        "vuejs",
        "filepond",
        "file",
        "upload",
        "drag",
        "drop",
        "browse",
        "image",
        "preview"
    ],
    "author": {
        "name": "PQINA",
        "url": "https://pqina.nl"
    },
    "scripts": {
        "build": "npm run build:lib && npm run build:module && npm run build:example",
        "build:lib": "vue-cli-service build --target lib --name vue-filepond lib/index.js --mode production && rm ./dist/demo.html",
        "build:module": "minicat lib/index.js | bannerjs -m > dist/vue-filepond.esm.js",
        "build:example": "minicat lib/index.js | bannerjs -m > example/src/assets/vue-filepond.js",
        "build:watch": "nodemon --watch lib -x \"npm run build\"",
        "watch": "nodemon --watch lib -x \"npm run build:example\"",
        "prepare": "npm run build"
    },
    "peerDependencies": {
        "filepond": ">=4.7.4 < 5.x",
        "vue": ">=3.0.5"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0",
        "babel-eslint": "^10.1.0",
        "bannerjs": "^1.0.7",
        "core-js": "^3.6.5",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^7.0.0-0",
        "filepond": "4.9.3",
        "minicat": "^1.0.0",
        "nodemon": "^2.0.2",
        "typescript": "^3.7.4",
        "uglify-js": "^3.12.5",
        "vue": "^3.0.5"
    }
}
